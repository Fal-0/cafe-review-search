{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "cafe-review-search.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/cafe-review-search.html",
      "dest": "/cafe-review-search.html"
    },
    {
      "src": "/",
      "dest": "/cafe-review-search.html"
    }
  ]
}

